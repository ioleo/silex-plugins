{
  "name": "@silexlabs/silex-plugins",
  "version": "0.0.0",
  "description": "Open architecture for Silex plugins",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
